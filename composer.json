{
    "name": "bharris/php-salesforce-rest-api",
    "description": "Library for interacting with SalesForce.com via the REST API",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "vlucas/phpdotenv": "^2.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Harris",
            "email": "bharris@ameronix.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "classmap": [
            "tests/BaseTest.php"
        ],
        "psr-0": {
            "Tests\\": "./tests"
        },
        "psr-4": {
            "Salesforce\\": "src/"
        }
    }
}
